<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite thread-count="5" name="Suite" parallel="tests">


	<groups>
		<run>
			<include name="master" />
			<include name="dataDriven" />
			<!--<include name="master"/>collection of every things -->
			<!--<include name="regression"/> only run particular file-->
			<!--<exclude name=""/> not run in group-->
		</run>
	</groups>

	<listeners>
		<listener class-name="rahulshetty.utilities.ExtentReportManager" />
	</listeners>


	<test name="chromebrowsertest">
		<parameter name="os" value="Windows" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="rahulshetty.testCases.TC001_UserRegister" />
			<class name="rahulshetty.testCases.TC002_LoginTest" />
			<class name="rahulshetty.testCases.TC003_LoginDDT" />
		</classes>
	</test> <!--Test -->


	<test name="edgebrowsertest">
		<parameter name="os" value="Windows" />
		<parameter name="browser" value="edge" />
		<classes>
			<class name="rahulshetty.testCases.TC001_UserRegister" />
			<class name="rahulshetty.testCases.TC002_LoginTest" />
			<class name="rahulshetty.testCases.TC003_LoginDDT" />
		</classes>
	</test> <!--Test -->


	<test name="firefoxbrowsertest">
		<parameter name="os" value="Windows" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="rahulshetty.testCases.TC001_UserRegister" />
			<class name="rahulshetty.testCases.TC002_LoginTest" />
			<class name="rahulshetty.testCases.TC003_LoginDDT" />
		</classes>
	</test> <!--Test -->

</suite> <!--Suite -->
